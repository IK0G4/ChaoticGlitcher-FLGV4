			lwing1topweld.C1 = Clerp(lwing1topweld.C1, CF(0, 0, 0-VisWing[4]), 0.4 / Animation_Speed)
			lwing1rightweld.C1 = Clerp(lwing1rightweld.C1, CF(0-VisWing[4], 0, 0), 0.4 / Animation_Speed)
			lwing1leftweld.C1 = Clerp(lwing1leftweld.C1, CF(0+VisWing[4], 0, 0), 0.4 / Animation_Speed)
			lwing2weld.C1 = Clerp(lwing2weld.C1, CF(6.10+VisWing[5]/2, -1, 0-VisWing[5]), 0.4 / Animation_Speed)
