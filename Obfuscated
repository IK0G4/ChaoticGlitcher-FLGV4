			rwing2topweld.C1 = Clerp(rwing2topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			rwing2rightweld.C1 = Clerp(rwing2rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing2leftweld.C1 = Clerp(rwing2leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing3weld.C1 = Clerp(rwing3weld.C1, CF(-9.50+0.35*COS(SINE / 32), -1, 0+1.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 + 16 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			rwing3topweld.C1 = Clerp(rwing3topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			rwing3rightweld.C1 = Clerp(rwing3rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing3leftweld.C1 = Clerp(rwing3leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing1weld.C1 = Clerp(lwing1weld.C1, CF(2.4, -1, 0+0.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 - 12 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			lwing1topweld.C1 = Clerp(lwing1topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			lwing1rightweld.C1 = Clerp(lwing1rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing1leftweld.C1 = Clerp(lwing1leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing2weld.C1 = Clerp(lwing2weld.C1, CF(6.10-0.25*COS(SINE / 32), -1, 0+1*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 - 14 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			lwing2topweld.C1 = Clerp(lwing2topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			lwing2rightweld.C1 = Clerp(lwing2rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing2leftweld.C1 = Clerp(lwing2leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing3weld.C1 = Clerp(lwing3weld.C1, CF(9.50-.35*COS(SINE / 32), -1, 0+1.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 - 16 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			lwing3topweld.C1 = Clerp(lwing3topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			lwing3rightweld.C1 = Clerp(lwing3rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing3leftweld.C1 = Clerp(lwing3leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			handleweld.C1 = Clerp(handleweld.C1, CF(0, -1, -1.5) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.4 / Animation_Speed)
		end
		if FULL == false then
			if TORSOVERTICALVELOCITY > 1 and HITFLOOR == nil then
				ANIM = "Jump"
				if ATTACK == false then
					if MODE ~= "Purged" then
						RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1.1) - 1)) * ANGLES(RAD(-20), RAD(0), RAD(0)), 3 / Animation_Speed)
						RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-40), RAD(0), RAD(20)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
