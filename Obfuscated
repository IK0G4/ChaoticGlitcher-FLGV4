	local sorb = CreateParta(m,1,1,"SmoothPlastic",BrickColor.random())
	CreateWeld(sorb,RightArm,sorb,0,1,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
