	function OverForm()
		ATTACK = true
		Rooted = true
		killcam=true
		BALL.Parent = Character
		XEYE.Parent = nil
		local SPIN = 0
		sick.TimePosition = 0
		SONG = 347151332
		PITCH = 1
		VOLUME = 4
		FACE = 404306534
		for i=0, 5, 0.1 / Animation_Speed do
			Swait()
			SPIN = SPIN + 25
			WACKYEFFECT({Time = 1 ,EffectType = "Swirl", Size = VT(1,1.5,1)*i*5, Size2 = VT(1,1.5,1)*i*5, Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0+i*2,0)*ANGLES(RAD(0),RAD(0+SPIN*2),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = 1 ,EffectType = "Swirl", Size = VT(1,1.5,1)*i*2.5, Size2 = VT(1,1.5,1)*i*2.5, Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0+i*1.5,0)*ANGLES(RAD(0),RAD(0+SPIN*2),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0+SPIN)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(90)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-90)) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		for i = 0, 25 do
			WACKYEFFECT({Time = math.random(10,100) ,EffectType = "Swirl", Size = VT(0,0,0), Size2 = VT(50,50,50)*MRANDOM(50/50,125/50), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,10,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = math.random(10,100) ,EffectType = "Swirl", Size = VT(0,0,0), Size2 = VT(25,25,25)*MRANDOM(50/50,125/50), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,5,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		end
		CreateSound(847061203,Torso,3,1,false)
		CreateSound(239000203,Torso,3,1,false)
		CreateSound(579687077,Torso,3,0.75,false)
		for i = 0, 72 do
			WACKYEFFECT({EffectType = "Sphere", Size = VT(1.1,1,1.1), Size2 = VT(1.1,188,1.1), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		end
		Speed = 35
		WACKYEFFECT({EffectType = "Wave", Size = VT(23,40,23), Size2 = VT(80,35,80), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-3,0), MoveToPos = nil, RotationX = 0, RotationY = 8, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		WACKYEFFECT({EffectType = "Sphere", Size = VT(35,35,35), Size2 = VT(80,80,80), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		text.Text = "Toxicity"
		ned.Text = "Toxicity"
		TEXTCOLOR = C3(0/255,103/255,0/255)
		PRT.Color = C3(0/255,103/255,0/255)
		EYE.Color = C3(0/255,255/255,0/255)
		EYE2.Color = C3(0/255,255/255,0/255)
		Hair.Color = C3(0/255,103/255,0/255)
		Hair2.Color = C3(0/255,255/255,0/255)
		MODE = "Overgrowth"
		MAINRUINCOLOR = BrickColor.new("Parsley green")
		SECONDRUINCOLOR  = BrickColor.new("Parsley green")
		RecolorThing(MAINRUINCOLOR,SECONDRUINCOLOR,"Neon","Neon",true)
		killcam=false
		ATTACK = false
		Rooted = false
	end

