			WACKYEFFECT({Time = 80,EffectType = "Sphere", Size = VT(0,0.55,0), Size2 = VT(30,0.55,30), Transparency = 0, Transparency2 = 1, CFrame = CF(HITPOS), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			CameraEnshaking(3,10)
			ManualAoE(HITPOS,15,20,45,75,false)
			for i=0, 0.5, 0.1 / Animation_Speed do
