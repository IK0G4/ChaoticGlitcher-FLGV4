		end
		par1.CFrame=RootPart.CFrame*CFrame.Angles(0,math.rad(lol),0)*CFrame.new(0,-3,-5)
		par2.CFrame=RootPart.CFrame*CFrame.Angles(0,math.rad(lol2),0)*CFrame.new(0,-3,-6.5)
		Humanoid.DisplayDistanceType = "None"
		Humanoid.PlatformStand = false
		for i,v in pairs(Character:GetChildren()) do
			if v:IsA("Shirt") or v:IsA("Pants") or v:IsA("ShirtGraphic") then
				v:Destroy()
			end
		end
		if MODE=="Sense" then
			for i,v in pairs(Outfit:GetDescendants()) do
				if v.Name == "C2" then
					v.Color = TEXTCOLOR
				elseif v.Name == "C" then
					v.Color = text.TextStrokeColor3
				elseif v:IsA("ParticleEmitter") then
					v.Color = ColorSequence.new(TEXTCOLOR)
				end
			end
		elseif MODE=="Nuclear" then
			for i,v in pairs(Outfit:GetDescendants()) do
				if v.Name == "C2" then
--					v.Color = C3(GetClientProperty(sick,"PlaybackLoudness")/1500,GetClientProperty(sick,"PlaybackLoudness")/1500,0)
				elseif v.Name == "C" then
				--	v.Color = C3(0,GetClientProperty(sick,"PlaybackLoudness")/1000,0)
				elseif v:IsA("ParticleEmitter") then
				--	v.Color = ColorSequence.new(C3(GetClientProperty(sick,"PlaybackLoudness")/1500,GetClientProperty(sick,"PlaybackLoudness")/1500,0))
				end
			end
		else
			for i,v in pairs(Outfit:GetDescendants()) do
				if v.Name == "C2" then
					v.Color = TEXTCOLOR
				elseif v.Name == "C" then
					v.Color = text.TextColor3
				elseif v:IsA("ParticleEmitter") then
					v.Color = ColorSequence.new(TEXTCOLOR)
				end
			end
		end
		for _, c in pairs(Torso:GetChildren()) do
			if c:IsA("JointInstance") or c.ClassName == "Sound" then
			else
				c:remove()
			end
		end
	end
	--//=================================\\
	--\\=================================//





	--//====================================================\\--
	--||			  		 END OF SCRIPT
	--\\====================================================//--
