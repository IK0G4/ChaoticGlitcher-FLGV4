			lwing3topweld.C1 = Clerp(lwing3topweld.C1, CF(0, 0, 0-VisWing[6]), 0.4 / Animation_Speed)
			lwing3rightweld.C1 = Clerp(lwing3rightweld.C1, CF(0-VisWing[6], 0, 0), 0.4 / Animation_Speed)
			lwing3leftweld.C1 = Clerp(lwing3leftweld.C1, CF(0+VisWing[6], 0, 0), 0.4 / Animation_Speed)
			handleweld.C1 = Clerp(handleweld.C1, CF(0, -1, -1.5) * ANGLES(RAD(0), RAD(0), RAD(0)), 3 / Animation_Speed) 
		elseif MODE=="LostHope" then    
			rwing1weld.C1 = Clerp(rwing1weld.C1, CF(2.4, -1, 0) * ANGLES(RAD(0), RAD(0 + 16 * SIN(SINE / 42.75)), RAD(0)), 0.4 / Animation_Speed)
			rwing1topweld.C1 = Clerp(rwing1topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 49.25)), 0.4 / Animation_Speed)
			rwing1rightweld.C1 = Clerp(rwing1rightweld.C1, CF(-1.1+0.2*SIN(SINE / 49.5), 0, 0), 0.4 / Animation_Speed)
			rwing1leftweld.C1 = Clerp(rwing1leftweld.C1, CF(1.1-0.2*SIN(SINE / 49.75), 0, 0), 0.4 / Animation_Speed)
			rwing2weld.C1 = Clerp(rwing2weld.C1, CF(6.10, -1, 0) * ANGLES(RAD(0), RAD(0 + 14 * SIN(SINE / 42.5)), RAD(0)), 0.4 / Animation_Speed)
			rwing2topweld.C1 = Clerp(rwing2topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 48.25)), 0.4 / Animation_Speed)
			rwing2rightweld.C1 = Clerp(rwing2rightweld.C1, CF(-1.1+0.2*SIN(SINE / 48.5), 0, 0), 0.4 / Animation_Speed)
			rwing2leftweld.C1 = Clerp(rwing2leftweld.C1, CF(1.1-0.2*SIN(SINE / 48.75), 0, 0), 0.4 / Animation_Speed)
			rwing3weld.C1 = Clerp(rwing3weld.C1, CF(9.50, -1, 0) * ANGLES(RAD(0), RAD(0 + 12 * SIN(SINE / 42.25)), RAD(0)), 0.4 / Animation_Speed)
			rwing3topweld.C1 = Clerp(rwing3topweld.C1, CF(0, 0, -1.1+0.15*SIN(SINE / 47.25)), 0.4 / Animation_Speed)
			rwing3rightweld.C1 = Clerp(rwing3rightweld.C1, CF(-1.1+0.15*SIN(SINE / 47.5), 0, 0), 0.4 / Animation_Speed)
			rwing3leftweld.C1 = Clerp(rwing3leftweld.C1, CF(1.1-0.15*SIN(SINE / 47.75), 0, 0), 0.4 / Animation_Speed)
