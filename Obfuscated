			lwing2topweld.C1 = Clerp(lwing2topweld.C1, CF(0, 0, -1.1+0.15*SIN(SINE / 45.25)), 0.4 / Animation_Speed)
			lwing2rightweld.C1 = Clerp(lwing2rightweld.C1, CF(-1.1+0.15*SIN(SINE / 45.5), 0, 0), 0.4 / Animation_Speed)
			lwing2leftweld.C1 = Clerp(lwing2leftweld.C1, CF(1.1-0.15*SIN(SINE / 45.75), 0, 0), 0.4 / Animation_Speed)
			lwing3weld.C1 = Clerp(lwing3weld.C1, CF(-9.50, -1, 0) * ANGLES(RAD(0), RAD(0 - 12 * SIN(SINE / 43.5)), RAD(0)), 0.4 / Animation_Speed)
			lwing3topweld.C1 = Clerp(lwing3topweld.C1, CF(0, 0, -1.1+0.15*SIN(SINE / 46.25)), 0.4 / Animation_Speed)
			lwing3rightweld.C1 = Clerp(lwing3rightweld.C1, CF(-1.1+0.15*SIN(SINE / 46.5), 0, 0), 0.4 / Animation_Speed)
			lwing3leftweld.C1 = Clerp(lwing3leftweld.C1, CF(1.1-0.15*SIN(SINE / 46.75), 0, 0), 0.4 / Animation_Speed)
			handleweld.C1 = Clerp(handleweld.C1, CF(0, -1, -1.5) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.4 / Animation_Speed)
		elseif MODE == "Devil" or MODE == "90s" or MODE == "Lunar" or MODE == "Hexa" then
			rwing1weld.C1 = Clerp(rwing1weld.C1, CF(-2.4, -1, 0+0.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 + 12 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			rwing1topweld.C1 = Clerp(rwing1topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			rwing1rightweld.C1 = Clerp(rwing1rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing1leftweld.C1 = Clerp(rwing1leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing2weld.C1 = Clerp(rwing2weld.C1, CF(-6.10+0.25*COS(SINE / 32), -1, 0+1*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 + 14 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			rwing2topweld.C1 = Clerp(rwing2topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			rwing2rightweld.C1 = Clerp(rwing2rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing2leftweld.C1 = Clerp(rwing2leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing3weld.C1 = Clerp(rwing3weld.C1, CF(-9.50+0.35*COS(SINE / 32), -1, 0+1.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 + 16 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			rwing3topweld.C1 = Clerp(rwing3topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
			rwing3rightweld.C1 = Clerp(rwing3rightweld.C1, CF(-1.1+0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			rwing3leftweld.C1 = Clerp(rwing3leftweld.C1, CF(1.1-0.2*SIN(SINE / 32), 0, 0), 0.4 / Animation_Speed)
			lwing1weld.C1 = rwing1weld.C1
			lwing1topweld.C1 = rwing1topweld.C1
			lwing1rightweld.C1 = rwing1rightweld.C1
			lwing1leftweld.C1 = rwing1leftweld.C1
			lwing2weld.C1 = rwing2weld.C1
			lwing2topweld.C1 = rwing2topweld.C1
			lwing2rightweld.C1 = rwing2rightweld.C1
			lwing2leftweld.C1 = rwing2leftweld.C1
			lwing3weld.C1 = rwing3weld.C1
			lwing3topweld.C1 = rwing3topweld.C1
			lwing3rightweld.C1 = rwing3rightweld.C1
			lwing3leftweld.C1 = rwing3leftweld.C1
			handleweld.C1 = Clerp(handleweld.C1, CF(0, -1, -1.5) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.4 / Animation_Speed)
		else
			rwing1weld.C1 = Clerp(rwing1weld.C1, CF(-2.4, -1, 0+0.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 + 12 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
			rwing1topweld.C1 = Clerp(rwing1topweld.C1, CF(0, 0, -1.1+0.2*SIN(SINE / 32)), 0.4 / Animation_Speed)
