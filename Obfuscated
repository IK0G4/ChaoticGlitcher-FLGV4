	basgui.Parent = Player.PlayerGui
	local fullscreenz = basgui.MainColorFrame
	local fullscreenz2 = basgui.BorderFrame
	local imgl2 = basgui.imgl2
	local techc = basgui.techc
	local circl = basgui.circl
	local ned = basgui.ned

